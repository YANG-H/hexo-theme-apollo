<div class="mobile-nav-panel">
	<i class="icon-reorder icon-large"></i>
</div>
<header id="header">
	<h1 class="blog-title">
		<a href="<%- config.root %>"><%= config.title %></a>
	</h1>
	<nav class="nav">
		<ul>
			<!--<% if (theme.menu.Home){ %>
				<li><a href="<%- theme.menu.Home %>" id="nav-home-btn" class="nav-icon" title="Home"></a></li>
			<% } %>

			<% if (theme.menu.Archives){ %>
				<li><a href="<%- theme.menu.Archives %>" id="nav-archives-btn" class="nav-icon" title="Archives"></a></li>
			<% } %> -->

			<% for (var i in theme.menu){ %>
				<li><a href="<%- theme.menu[i] %>"><%= i %></a></li>
			<% } %>

			<li><a id="nav-search-btn" class="nav-icon" title="Search"></a></li>

			<% if (theme.rss){ %>
				<li><a href="<%- theme.rss %>" id="nav-rss-link" class="nav-icon" title="RSS Feed"></a></li>
			<% } %>
			
			<% if (theme.contact.twitter){ %>
				<li><a href="https://twitter.com/<%- theme.contact.twitter %>" id="nav-twitter-btn" class="nav-icon" title="Follow @<%- theme.contact.twitter %>"></a></li>
			<% } %>
			
			<% if (theme.contact.facebook) { %>
				<li><a class="nav-icon" href="https://www.facebook.com/<%- theme.contact.facebook %>" id="nav-facebook-btn" title="Follow me on Facebook"></a></li>
			<% } %>
		</ul>
	</nav>
	<div id="search-form-wrap">
		<%- search_form({button: '&#xF002;'}) %>
	</div>
</header>